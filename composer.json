    "keywords": [
        "gidato",
        "filesystem",
        "temporary"
    ],
    "license": "MIT",
    "require": {
        "gidato/filesystem-model": "^0",
        "ramsey/uuid": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Gidato\\TemporaryPath\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
